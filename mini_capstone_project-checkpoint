   "source": [
    "Location Analysis:\n",
    "\n",
    "Where do most crimes occur based on the \"Location\" column?\n",
    "\n",
    "\n",
    "import pandas as pd\n",
    "import pymysql\n",
    "conn = pymysql.connect(\n",
    "    host='localhost',\n",
    "    user='root',\n",
    "    password='Ganesh@!2000',\n",
    "    database='bojju7')\n",
    "query = \"SELECT * FROM crime_data\" \n",
    "crime_data = pd.read_sql(query, conn)\n",
    "conn.close()\n",
    "crime_location_counts = crime_data['Location'].value_counts()\n",
    "most_common_location = crime_location_counts.idxmax()\n",
    "highest_crime_count = crime_location_counts.max()\n",
    "print(\"Most common location for crimes:\", most_common_location)\n",
    "print(\"Number of reported crimes at this location:\", highest_crime_count)\n"
   ]
