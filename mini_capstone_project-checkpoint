    "Where are the geographical hotspots for reported crimes?\n",
    "\n",
    "\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import pymysql\n",
    "conn = pymysql.connect(\n",
    "    host='localhost',\n",
    "    user='root',\n",
    "    password='Ganesh@!2000',\n",
    "    database='bojju7')\n",
    "query = \"SELECT * FROM crime_data\" \n",
    "crime_data = pd.read_sql(query, conn)\n",
    "conn.close()\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.scatter(crime_data['LON'], crime_data['LAT'], alpha=0.5, marker='*', color='BLUE')\n",
    "plt.title('Reported Crime Hotspots')\n",
    "plt.xlabel('LON')\n",
    "plt.ylabel('LAN')\n",
    "plt.grid(True)\n",
